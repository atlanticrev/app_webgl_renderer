{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "./",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": []
}
